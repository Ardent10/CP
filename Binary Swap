#include<bits/stdc++.h>
using namespace std;

int main(){
      
	  string a,b;
	  cin>>a>>b;
      
       int count=0;

	    for(int i=0;i<b.length();i++){
		 
		    if(b[i]!=a[i]) count++;   
	   }
       
	   if(count%2==0)
	   {
		   cout<<(count/2);
	   }
	   else cout<<"-1";


return 0;
}
