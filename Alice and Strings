#include<bits/stdc++.h>
using namespace std;

int main()
{
      string A,B;
      cin>>A>>B; 

       int flag=0;
      

      if(A.length()!=B.length())
      {
         cout<<"NO";
      }
      else
      {
           for (int i = 0; i<A.length(); i++)
           {
              if(A[i]>B[i] && A[i]=='z') 
              {
                 flag =1;
                 cout<<"NO";
                 break;
              }
           }        
          if(flag==0) cout<<"YES";
      }
     


return 0;
}

       
